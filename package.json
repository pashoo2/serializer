{
  "name": "@pashoo2/serializer",
  "version": "0.0.3",
  "description": "An utility class for data serialization with an ability to serialize functions",
  "main": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "es/**/*"
  ],
  "engines": {
    "node": ">=10.4.0"
  },
  "engineStrict": true,
  "scripts": {
    "test": "jest --passWithNoTests",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc --project ./tsconfig.build.json",
    "fix": "gts fix",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run test",
    "preversion": "npm run lint",
    "version": "npm run fix && git add -A src",
    "postversion": "git push && git push --tags",
    "docs": "typedoc --out docs src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pashoo2/serializer.git"
  },
  "keywords": [
    "typescript",
    "serialization",
    "function serialization",
    "function export",
    "function import",
    "data serialization",
    "data parsing"
  ],
  "author": "Pavel Akulich akulich.pavel.stage@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pashoo2/serializer/issues"
  },
  "homepage": "https://github.com/pashoo2/serializer/#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@types/estree": "0.0.50",
    "@types/jest": "^26.0.23",
    "@types/node": "^10.04.0",
    "babel-jest": "^27.0.6",
    "gts": "^3.1.0",
    "jest": "^27.0.3",
    "ts-jest": "27.0.2",
    "typedoc": "^0.21.2",
    "typedoc-plugin-markdown": "^3.10.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@pashoo2/utils": "0.0.1",
    "sval": "^0.4.8"
  }
}
